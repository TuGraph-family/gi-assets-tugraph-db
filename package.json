{
  "name": "@tugraph/gi-assets-tugraph-db",
  "version": "0.6.2",
  "description": "G6VP Assets for TuGraph-DB",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "G6VP",
    "gi-asstes",
    "TuGraph"
  ],
  "license": "MIT",
  "files": [
    "dist",
    "es",
    "lib"
  ],
  "scripts": {
    "start": "father build --watch",
    "build:es": "father build",
    "build:umd": "webpack  --mode production  -c ./webpack.config.js",
    "build:umd:watch": "webpack  --mode production  -c ./webpack.config.js --env watch=true",
    "build": "father build && npm run build:umd",
    "serve": "http-server dist -p 9527",
    "demo": "umi dev"
  },
  "dependencies": {
    "@antv/g2plot": "^2.4.31",
    "@antv/g6": "4.x",
    "@antv/gi-sdk": "latest",
    "@antv/graphin": "2.x",
    "ace-gremlin-editor": "^1.0.2",
    "antd": "4.x",
    "deepmerge": "^4.3.1",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.2",
    "react": "17.x",
    "react-color": "^2.19.3",
    "react-dom": "17.x",
    "umi-request": "^1.4.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "father": "^2.30.6",
    "http-server": "^14.1.1",
    "less-loader": "^11.1.3",
    "mini-css-extract-plugin": "^2.7.2",
    "ts-loader": "^9.4.2",
    "typescript": "^4.1.2",
    "umi": "3.x",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "use-immer": "^0.9.0"
  },
  "authors": [
    "https://github.com/pomelo-nwu",
    "https://github.com/baizn"
  ],
  "pnpm": {
    "overrides": {
      "@antv/gi-sdk": "latest"
    }
  },
  "repository": "https://github.com/TuGraph-family/tugraph-db-browser.git"
}